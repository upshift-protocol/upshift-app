{
  "name": "upshift-earn-app",
  "version": "1.0.0",
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "dev": "NEXT_PUBLIC_DEV=1 next dev",
    "dev:local": "NEXT_PUBLIC_NETWORK=localhost pnpm dev",
    "dev:referral": "NEXT_PUBLIC_REFERRALS_ENABLED=true pnpm dev",
    "dev:lombard": "NEXT_PUBLIC_INSTANCE=lombard pnpm dev",
    "dev:treehouse": "NEXT_PUBLIC_INSTANCE=treehouse pnpm dev",
    "dev:ethena": "NEXT_PUBLIC_INSTANCE=ethena NEXT_PUBLIC_REFERRALS_ENABLED=true pnpm dev",
    "dev:avax": "NEXT_PUBLIC_INSTANCE=avax pnpm dev",
    "build": "next build",
    "start": "next start",
    "build-prod": "run-s clean build",
    "clean": "rimraf .next .swc out",
    "lint": "next lint",
    "format": "next lint --fix && prettier '**/*.{json,yaml}' --write --ignore-path .gitignore",
    "check-types": "tsc --noEmit --pretty",
    "prepare": "husky install",
    "analyze": "ANALYZE=true next build",
    "link:sdk": "pnpm link ../js-sdk/packages/sdk",
    "unlink:sdk": "pnpm unlink @augustdigital/sdk",
    "reset": "rm -rf node_modules pnpm-lock.yaml out .next && pnpm i"
  },
  "dependencies": {
    "@augustdigital/sdk": "1.6.6",
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.5",
    "@mui/icons-material": "^6.1.6",
    "@mui/material": "^6.1.6",
    "@mui/x-data-grid": "^7.8.0",
    "@next/third-parties": "^15.0.3",
    "@tanstack/react-query": "^5.49.2",
    "chart.js": "^4.4.5",
    "classnames": "^2.5.1",
    "ethers": "^6.13.2",
    "google-auth-library": "^9.14.2",
    "google-spreadsheet": "^4.1.4",
    "next": "^15.0.3",
    "next-seo": "^6.6.0",
    "nextjs-progressbar": "^0.0.16",
    "react": "^18.2.0",
    "react-chartjs-2": "^5.2.0",
    "react-dom": "^18.2.0",
    "react-toastify": "^10.0.5",
    "styled-jsx-plugin-postcss": "^4.0.1",
    "viem": "^2.16.5",
    "wagmi": "^2.10.8"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^15.0.3",
    "@tanstack/react-query-devtools": "^5.54.1",
    "@types/node": "^22.9.0",
    "@types/react": "^18.2.48",
    "@typescript-eslint/eslint-plugin": "^8.13.0",
    "cross-env": "^7.0.3",
    "cssnano": "^7.0.6",
    "eslint": "^8.57.1",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-config-next": "^15.0.3",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-tailwindcss": "^3.14.1",
    "eslint-plugin-unused-imports": "^4.1.4",
    "husky": "^9.1.6",
    "lint-staged": "^15.2.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.2.4",
    "rimraf": "^6.0.1",
    "typescript": "^5.3.3"
